{
  "manifest_version": 3,

  "name": "CLOSE ALL THE 9GAG !!1",
  "description": "closes all 9gag (and other) tabs every X minutes",
  "version": "0.0.4",
  
  "background": {
    "service_worker": "background.js"
  },
  
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "16":  "img/icons/icon16.png",
    "48":  "img/icons/icon48.png",
    "128": "img/icons/icon128.png" 
  },
  "permissions": [
    "tabs",
    "storage",
    "alarms"
  ]
}